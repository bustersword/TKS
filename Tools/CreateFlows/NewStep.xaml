<Window x:Class="CreateFlows.NewStep"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NewStep" Height="202" Width="204" WindowStartupLocation="CenterOwner">
    <Grid Margin="0,0,0,-1">
        <TextBox HorizontalAlignment="Left" x:Name="txtAssemblyName"  Height="23" Margin="10,66,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="176"/>
        <Label x:Name="lblTitle" Content="程序集名" HorizontalAlignment="Left" Margin="10,23,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.328,-0.308"/>
        <Button Content="OK" x:Name="btnOK" Click="btnOK_Click_1" HorizontalAlignment="Left" Margin="10,118,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Cancel" x:Name="btnCancel" Click="btnCancel_Click_1" HorizontalAlignment="Left" Margin="111,118,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
